#include <iostream>

using namespace std;

int main()
{
    int senha;
    senha = 0;
    printf("Digite a senha de 4 digitos: ");
    while (senha != 1234) {
        scanf("%d", &senha);
        if (senha != 1234) {
            printf("\nSenha incorreta!Tente novamente:\n");
        }
    }
    printf("\nBem vindo!\n");
    return 0;
}
