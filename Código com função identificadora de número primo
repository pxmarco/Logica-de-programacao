#include <iostream>

using namespace std;

int main()
{
    int i, numero, contador;
    contador = 0;
    printf ("Digite um numero:\n");
    scanf ("%d", &numero);
    for(i = 1; i <= numero; i++){
        if (numero % i == 0) {
            contador ++;
            }}

    if (contador == 2){
        printf ("\nO numero e primo");
    }
    else{
        printf ("\nO numero nao e primo");
    }
    return 0;
}
